        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- umeng   start  2016-07-12 -->
        <meta-data android:value="5784aa8ee0f55a4c48000c99" android:name="UMENG_APPKEY"></meta-data>
        <meta-data android:value="${CHANNEL}" android:name="UMENG_CHANNEL"/>
        <!-- umeng   end -->

        <meta-data android:name="CHANNEL" android:value="${CHANNEL}"/>
    </application>

</manifest>